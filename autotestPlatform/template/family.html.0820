<style type="text/css">

h2{
	font-weight:bold;
}

h3{
	font-weight:bold;
}

.sec h3{
	text-align:center;
}

.plist{
	padding:30px 0;
	width:1000px;
	overflow:hidden;
	margin:auto;
}
.plist .sec{
	width:200px;
	padding:0 50px 0 0;
	overflow:hidden;
	float:left;
}
.plist .sec_last{
	padding:0;
}
.plist .sec h2{
	font-size:20px;
	padding:0 0 8px 0;
}
.plist .sec h2 a{
	color:#5f5f5f;
	text-decoration:none;
}
.plist .sec h2 a:hover{
	text-decoration:underline;
}
.plist .sec p{
	color:#666;
	font-size:14px;
}
.home_foot{
	padding:20px 0 0 0;
	width:100%;
	overflow:hidden;
	height:80px;
	text-align:center;
	color:#999;
	background:#f5f5f5;
}
.ilist{
	width:1000px;
	margin:auto;
	overflow:hidden;
	padding:20px 0;
	border-top:1px dashed #CCC;
}
.ilist .sec{
	float:left;
	width:250px;
}
.ilist .sec .icon{
	float:left;
	width:48px;
	height:48px;
}
.ilist .sec .list1{
	background:url(/static/images/quality.png) no-repeat;
}
.ilist .sec .list2{
	background:url(/static/images/ass.png) no-repeat;
}
.ilist .sec .list3{
	background:url(/static/images/robot.png) no-repeat;
}
.ilist .sec .list4{
	background:url(/static/images/index.png) no-repeat;
}
.ilist .sec .title{
	float:left;
	line-height:48px;
	padding-left:10px;
	font-size:12px;
	color:#7c7c7c;
}
.ilist .sec .title a{
	color:#7c7c7c;
	text-decoration:none;
}
.ilist .sec .title a:hover{
	text-decoration:underline;
}
.permissioninfo_entrance{
    position: fixed;
    right: 0;
    bottom: 100px;
    text-align: center;
    z-index: 999;
}
.permissioninfo_entrance a{
    display: block;
    width: 14px;
    padding: 22px 10px 5px 10px;
    border: 1px solid #CCC;
    text-align: center;
    background: #549002;
    color: #000;
    text-decoration: none;
    line-height: 14px;
    background:#e9eef2 url(/pharos/resource/images/bg_entranceapply.png) 8px 3px no-repeat;
}
.permissioninfo_list{
    width: 600px;
    border: 3px solid #757374;
    background: #FFF;
    position: fixed;
    top: 100px;
    left: 200px;
    z-index: 999;
    display: none;
    padding: 15px;
}
.permissioninfo_list h2{
    font-size: 14px;
    position: relative;
    padding: 10px;
    border-bottom: 1px solid #CCC;
}
.permissioninfo_list a{
    position: absolute;
    right:10px;
    top: 10px;
    text-decoration: none;
    font-size: 12px;
    font-weight: normal;
}
table.permission{
    font: 12px "微软雅黑";
    border-collapse: collapse;
}
table.permission th{
    padding: 8px 10px;
    text-align: left;
    border-bottom: 1px solid #CCC;
}
table.permission td{
    padding: 8px 10px;
    border-bottom: 1px solid #CCC;
}
.pmtips{
    color: #000;
    padding: 8px 10px;
    border-bottom: 1px solid #CCC;
    font: 12px "微软雅黑";
}


.clear:after {
    clear: both;
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
}
nav{
	display:inline-block;
	border:1px solid #505255;
	border-bottom: 1px solid #282C2F;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	margin:50px;
	-webkit-box-shadow:1px 1px 3px #292929;
    -moz-box-shadow:1px 1px 3px #292929;
}
li{
	list-style:none;
	float:left;
	border-right: 1px solid #2E3235;
	position: relative;
	/*background: -moz-linear-gradient(top, #fff, #555D5F 2% ,#555D5F  50%,#3E4245 100%);
	background: -webkit-gradient(linear, 0 0, 0 100%, from(#fff), color-stop(2%, #555D5F), color-stop(50%, #555D5F),to(#3E4245));*/
	#background:#555D5F;
	background:#17263B;
}
li:hover{
	/*background: -moz-linear-gradient(top, #fff, #3E4245 2% ,#555D5F  80%,#555D5F 100%);
	background: -webkit-gradient(linear, 0 0, 0 100%, from(#fff), color-stop(2%, #3E4245), color-stop(80%, #3E4245),to(#555D5F));*/
	#background:#3E4245;
	#background:#999E9F;
	background:#5E7692;
	-moz-transition: background 1s ease-out;
	-webkit-transition: background 1s ease-out;
}
li a{
	display:block;
	height:60px;
	line-height:60px;
	padding:0 30px;
	font-size:16px;
	font-weight:bold;
	color:#fff;
	text-shadow: 0px -1px 0px #000;
	text-decoration:none;
	white-space:nowrap;
	#border-left: 1px solid #999E9F;
    #border-top: 1px solid #999E9F;
	-moz-border-top-left-radius: 2px;
	-webkit-border-top-left-radius: 2px;
	
	z-index:100;
}
li > a{
	position:relative;
}
li.first a{
	-moz-border-radius-topleft: 4px;
	-moz-border-radius-bottomleft: 4px;
	-webkit-border-top-left-radius: 4px;
	-webkit-border-bottom-left-radius: 4px;
}
li.last{
	border-right: 0 none;
}


dl{
	position:absolute;
	display:block;
	top:60px;
	left: -25px;
	
	width:165px;
	
	background:#222222;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	
	-webkit-box-shadow:1px 1px 3px #292929;
    -moz-box-shadow:1px 1px 3px #292929;
	
	z-index:10;
            
}
li:hover dl{
	top:70px;
	display:block;
	width:145px;
	padding:10px;
}
dl a{
	background:transparent;
	border:0 none;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	-moz-transition: background 0.5s ease-out;
	-webkit-transition: background 0.5s ease-out;
	
	z-index:50;
	
	font-weight:normal;
}
dl a:hover{
	color:#FFF;
	#background:#999E9F;
	background:#5E7692;
	-moz-transition: background 0.3s ease-in-out, color 0.3s ease-in-out;
	-webkit-transition: background 0.3s ease-in-out, color 0.3s ease-in-out;
}
dd{
	margin-top:-60px;
	opacity:0;
	width:145px;
	-webkit-transition-property:all;
	/*-webkit-transition-timing-function: cubic-bezier(5,0,5,0);*/
	-moz-transition-property: all;
	/*-moz-transition-timing-function: cubic-bezier(5,0,5,0);*/
	/*-webkit-transition-delay:5s;
	-moz-transition-delay:5s;*/
}
li:hover dd{
	margin-top:0;
	opacity:1;
}
li dd:nth-child(1){
	-webkit-transition-duration: 0.1s;
	-moz-transition-duration: 0.1s;
}
li dd:nth-child(2){
	-webkit-transition-duration: 0.2s;
	-moz-transition-duration: 0.2s;
}
li dd:nth-child(3){
	-webkit-transition-duration: 0.3s;
	-moz-transition-duration: 0.3s;
}
li dd:nth-child(4){
	-webkit-transition-duration: 0.4s;
	-moz-transition-duration: 0.4s;
}
dt{
	display:none;
	margin-top:-25px;
	padding-top:15px;
	height:10px;
}
li:hover dt{
	display:block;
}
.Darrow{
	float:right;
	margin:38px 10px 0 0;
	border-width:5px;
	border-color:#FFF transparent transparent transparent;
	border-style:solid;
	width:0;
	height:0;
	line-height:0;
	overflow:hidden;
	
	cursor:pointer;
	
	text-shadow: 0px -1px 0px #000;
	
	-webkit-box-shadow:0px -1px 0px #000;
    -moz-box-shadow:0px -1px 0px #000;
}
.arrow{
	margin:0 auto;
	margin-top:-5px;
	display:block;
	width:10px;
	height:10px;
	background:#222222;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
}

